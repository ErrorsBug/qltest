<section class="topic-section list-section">
    <h1>
        话题
    </h1>
    <ul class="topics" id="topics">
        {{#each topics}}
        <li class="topic" data-id="{{id}}" log-business_id="{{id}}" log-name="{{topic}}"  log-business_type="topic">
            <span>
                    <img src="{{backgroundUrl}}" alt="">
                </span>
            <section>
                <h2 class="multi-elli">{{{topic}}}</h2>
                <p>
                    <span>{{dateFormat startTime 'MM-dd hh:mm'}}</span>
                    <span>{{browseNum}}次学习</span>
                </p>
            </section>
        </li>
        {{/each}}
    </ul>
    <div class="loading-next">加载数据中...</div>
    <div class="no-more">已全部加载完毕</div>
</section>