<div class="gift-head">
    {{#compare giftGet.getResultCode "==" "selfDo"}}
        {{#compare gift.remaindNum ">=" 0}}
            <span class="gift-status">剩余{{gift.remaindNum}}份</span>
        {{/compare}} 
        {{#compare gift.remaindNum "<" 0}}
            <span class="gift-status">赠礼已被领取完毕</span>
        {{/compare}}
        {{#if gift.IsOverTime}}
            <div class="gift-tips">赠礼已过期</div>
        {{else}}
            <div class="gift-tips">赠礼将于{{gift.overTime}}天后过期</div>
        {{/if}}
    {{/compare}}
    {{#compare giftGet.getResultCode "==" "overTime"}}
        <span class="gift-status">赠礼已过期</span>
        <div class="gift-tips">下次记得早点来哟~</div>
    {{/compare}}
    {{#compare giftGet.getResultCode "==" "giftEmpty"}}
        <span class="gift-status">赠礼已被领取完毕</span>
        {{#compare gift.remaindNum ">=" 0}}
            {{#if gift.IsOverTime}}
                <div class="gift-tips">赠礼已过期</div>
            {{else}}
                <div class="gift-tips">赠礼将于{{gift.overTime}}天后过期</div>
            {{/if}}
        {{/compare}}
        {{#compare gift.remaindNum "<" 0}}
            <div class="gift-tips">下次记得早点来哟~</div>
        {{/compare}}
    {{/compare}}
    {{#compare giftGet.getResultCode "==" "alreadyGetGift"}}
            <span class="gift-status">已成功领取赠礼</span>
            <div class="gift-tips">请在「 我的购买记录 」中查看</div>
    {{/compare}}
    {{#compare giftGet.getResultCode "==" "getByOthers"}}
        <span class="gift-status">该礼品已被他人领取</span>
    {{/compare}}
    {{#compare giftGet.getResultCode "==" "success"}}
        <span class="gift-status">已成功领取赠礼</span>
        <div class="gift-tips">请在「 我的购买记录 」中查看</div>
    {{/compare}}

</div>
<section class="gift-box ">
    <div class="gift-top">
        <span class="gift-top-title">赠礼</span>
        {{#compare channelInfo.chargeType "==" "absolutely"}}
            <span class="gift-top-money">￥{{money-format gift.price}}</span>
        {{/compare}}
        {{#compare channelInfo.chargeType "==" "flexible"}}
            <span class="gift-top-money">￥{{money-format gift.price}}/{{gift.month}}个月</span>
        {{/compare}}
        
        
    </div>
    <div class="gift-mid multi-elli">
        <span>
        {{channelname}}</span>
    </div>
    <div class="gift-bottom">
        <div>系列课类型  <span>{{channeltype}}</span></div>
        <div>直播间  <span>{{livename}}</span></div>
            <i class="{{#compare giftGet.getResultCode "!=" "selfDo"}} receive-icon {{/compare}} {{#if no_receive}} on {{/if}} "></i>
    </div>
</section>
<div class="gift-btn-box">
      {{#if gift.isSelf}}
        {{#if gift.IsOverTime}}
            <a class="gift-btn none" attr-href="javascript:;">赠送给好友</a>
            <a class="gift-btn-more none" href="javascript:;">还剩{{gift.remaindNum}}份赠礼未领取，点击群发</a>
        {{else}}
            {{#compare gift.remaindNum ">=" 1}}
                <a class="gift-btn" id="gift-btn" href="javascript:;" attr-href="/wechat/page/channel/giftsend/">赠送给好友</a>
             {{/compare}}
            {{#compare gift.remaindNum ">=" 2}}
                <a class="gift-btn-more" href="/wechat/page/channel/giftGroupsend/{{gift.id}}">还剩{{gift.remaindNum}}份赠礼未领取，点击群发</a>
            {{/compare}}
            {{#compare gift.remaindNum "<=" 0}}
                <a class="gift-btn none" attr-href="javascript:;">赠送给好友</a>
                <a class="gift-btn-more none" href="javascript:;">还剩{{gift.remaindNum}}份赠礼未领取，点击群发</a>
            {{/compare}}
        {{/if}}
        
     {{else}}  
        <a class="gift-btn" href="javascript:void(0)">
        {{#compare giftGet.getResultCode "==" "giftEmpty"}}继续{{/compare}}前往系列课</a>
        <a class="gift-btn-more" href="/liveCenter/index.htm">查看其他话题</a>
     {{/if}}
</div>